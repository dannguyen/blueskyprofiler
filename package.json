{
	"name": "bluesky-profiler-svelte",
	"version": "0.0.2",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"gh-pages": "npm run build && git checkout gh-pages && rm -rf docs && cp -r build docs && touch docs/.nojekyll && git add docs && git commit -m \"deploy $(date -u +\"%Y-%m-%dT%H:%M:%SZ\")\" && git push -u origin gh-pages && git checkout main"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-svg-core": "latest",
		"@fortawesome/free-regular-svg-icons": "latest",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.17.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"numeral": ">=2.0.6",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.9",
		"typescript": "^5.0.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"@fortawesome/free-regular-svg-icons": "^6.7.2"
	}
}
